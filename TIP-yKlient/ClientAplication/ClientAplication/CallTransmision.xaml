<Window x:Class="ClientAplication.CallTransmision"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientAplication"
        mc:Ignorable="d"
        Title="CallTransmision" Height="200" Width="300"
         ResizeMode="NoResize" Background="#F5F5F6" AllowsTransparency="True" WindowStyle="None"
        >
    <Grid>
        <StackPanel Margin="30">
        <TextBlock x:Name="stringWithName" Style="{StaticResource SubTitle}" HorizontalAlignment="Center" Margin="0,0,0,20"/>
            <TextBlock x:Name="timeTextBlock" HorizontalAlignment="Center" Style="{StaticResource SubTitle}" FontSize="10" Margin="0,0,0,20"/>
            <TextBlock x:Name="callIco" Foreground="Red" MouseLeftButtonDown="callDisconectTextboxaction" Text="&#xE0B1;" Style="{StaticResource IcoRight}" HorizontalAlignment="Center"   ></TextBlock>
        </StackPanel>
    </Grid>
</Window>
